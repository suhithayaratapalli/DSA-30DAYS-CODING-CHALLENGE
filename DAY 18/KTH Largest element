DAY 18- LC 703

class KthLargest {
    PriorityQueue<Integer> minHeap;
    int k;
    public KthLargest(int k, int[] nums) {
        minHeap=new PriorityQueue<>(k);
        this.k=k;
        for(int x:nums){
            add(x);
        }
}
    
    public int add(int val) {
        minHeap.add(val);
        if(minHeap.size()>k)
        minHeap.remove();
        return minHeap.peek();
        
    }
}

        
