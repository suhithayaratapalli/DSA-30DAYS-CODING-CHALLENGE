DAY 23-LC

class Solution {
    public int maxDepth(TreeNode root) {
        return dfs(root);    
    }
    public static int dfs(TreeNode root){
        if(root==null){
            return 0;
        }
        int left=dfs(root.left)+1;
        int right=dfs(root.right)+1;
        return Math.max(left,right);
    }
}
