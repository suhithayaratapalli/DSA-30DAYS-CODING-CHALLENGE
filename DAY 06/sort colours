class Solution {
        public void sortColors(int[] nums) {
            int zero = 0, one = 0, two = 0;
            for (int n : nums) {
                if (n == 0) zero++;
                else if (n == 1) one++;
                else two++;
            }

            for (int i = 0; i < nums.length; i++) {
â€¦            }
        }                                                                    
}
