DAY 12 - LC 278

public class Solution extends VersionControl {
    public int firstBadVersion(int n) {
        int left=1;
        int right=n;
        while(left<right){
            int mid=left+(right-left)/2;
            if(isBadVersion(mid)){
                 right=mid;
            }
            else{
                left=mid+1;
            }
        }
        if(left==right&&isBadVersion(left)){
            return left;
        }
        return -1;
    }
}

