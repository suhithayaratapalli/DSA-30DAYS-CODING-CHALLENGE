DAY 10 - LC 680 

class Solution {
        public boolean isPalindrome(String s, int left, int right) {
            while (left < right) {
                if (s.charAt(left) != s.charAt(right)) return false;
                left++;
                right--;
            }
            return true;
        }

â€¦
            return true;
        }
                                                                                                     
}
