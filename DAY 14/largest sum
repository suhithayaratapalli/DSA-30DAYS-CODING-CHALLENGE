DAY 14 - Largest sum

class Solution {
    public int splitArray(int[] nums, int k) {
        int n=nums.length;
        int low=0;
        int high=0;
        for(int i=0;i<n;i++){
            high+=nums[i];
            low=Math.max(low,nums[i]);
        }
        while(low<=high){
â€¦                st++;
                pages=nums[i];
            }
        }
        return st<=k;
    }
}
